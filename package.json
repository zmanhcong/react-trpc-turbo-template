{
  "name": "react-trpc-turbo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "npm@9.9.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "start:backend": "node apps/api/dist/server.js",
    "type": "turbo run type"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/database": "*",
    "@repo/typescript-config": "*",
    "eslint": "^9.19.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "turbo": "latest"
  }
}